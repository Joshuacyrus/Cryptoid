<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	android:id="@+id/widget30"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	xmlns:android="http://schemas.android.com/apk/res/android">
	
	<Button
		android:id="@+id/mainpicture"
		android:background="@drawable/mainpicture"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:layout_marginTop="20px"
		android:layout_marginBottom="30px"
		
		>
	</Button>
	

	<Button
		android:id="@+id/button_algorithms"
		android:layout_width="162px"
		android:layout_height="wrap_content"
		android:text="Algorithms and Codings"
		android:layout_gravity="center"
		>
	</Button>
	<Button
		android:id="@+id/button_cryptanalysis"
		android:layout_width="162px"
		android:layout_height="wrap_content"
		android:text="Cryptanalysis"
		android:layout_gravity="center">
	</Button>	
</LinearLayout>